        <include refid="Base_Column_List"/>
        from
        func_switch_config
        where 1=1
        <include refid="Base_Condition_Sql"/>
        <include refid="Query_Params_Condition_Sql"/>
        <if test="limit != -1">
            limit #{offset,jdbcType=BIGINT},#{limit,jdbcType=BIGINT}
        </if>
    </select>

