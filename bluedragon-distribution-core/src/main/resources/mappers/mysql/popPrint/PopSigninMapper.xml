	<insert id="insert" parameterType="PopSignin">
		INSERT INTO
		POP_SIGNIN(QUEUE_NO,THIRD_WAYBILL_CODE,CREATE_USER_CODE,CREATE_USER,OPERATE_TIME,EXPRESS_CODE,EXPRESS_NAME,CREATE_SITE_CODE,CREATE_SITE_NAME,CREATE_TIME,UPDATE_TIME,YN)
		VALUES(
		#{queueNo,jdbcType=VARCHAR},
		#{thirdWaybillCode,jdbcType=VARCHAR},
		#{createUserCode,jdbcType=INTEGER},
		#{createUser,jdbcType=VARCHAR},
		#{operateTime},
		#{expressCode,jdbcType=VARCHAR},
		#{expressName,jdbcType=VARCHAR},
		#{createSiteCode,jdbcType=INTEGER},
		#{createSiteName,jdbcType=VARCHAR},
