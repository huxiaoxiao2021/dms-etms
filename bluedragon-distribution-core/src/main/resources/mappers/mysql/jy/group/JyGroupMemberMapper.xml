<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.jd.bluedragon.distribution.jy.dao.group.JyGroupMemberDao">

	<!-- 可根据自己的需求，是否要使用 -->
    <resultMap type="com.jd.bluedragon.distribution.jy.group.JyGroupMemberEntity" id="jyGroupMemberMap">
        <result property="id" column="id"/>
        <result property="memberCode" column="member_code"/>
        <result property="refGroupCode" column="ref_group_code"/>
        <result property="refSignRecordId" column="ref_sign_record_id"/>
        <result property="userCode" column="user_code"/>
        <result property="userName" column="user_name"/>
        <result property="jobCode" column="job_code"/>
        <result property="masterFlag" column="master_flag"/>
        <result property="status" column="status"/>
        <result property="orgCode" column="org_code"/>
        <result property="siteCode" column="site_code"/>
        <result property="createUser" column="create_user"/>
        <result property="createUserName" column="create_user_name"/>
        <result property="updateUser" column="update_user"/>
        <result property="updateUserName" column="update_user_name"/>
        <result property="createTime" column="create_time"/>
        <result property="updateTime" column="update_time"/>
        <result property="yn" column="yn"/>
        <result property="ts" column="ts"/>
    </resultMap>

    <sql id="Base_Column_List">
        id,
        member_code,
        ref_group_code,
        ref_sign_record_id,
        user_code,
        user_name,
        job_code,
        master_flag,
        status,
        org_code,
        site_code,
        create_user,
        create_user_name,
        update_user,
        update_user_name,
        create_time,
        update_time,
        yn,
        ts
    </sql>

    <insert id="insert" parameterType="JyGroupMemberEntity">
        INSERT INTO jy_group_member (
                            id,
                            member_code,
                            ref_group_code,
                            ref_sign_record_id,
                            user_code,
                            user_name,
                            job_code,
                            master_flag,
                            status,
                            org_code,
                            site_code,
                            create_user,
                            create_user_name,
                            update_user,
                            update_user_name,
                            create_time,
                            update_time,
                            yn,
                            ts
                    ) VALUES (
                            #{id},
                            #{memberCode},
                            #{refGroupCode},
                            #{refSignRecordId},
                            #{userCode},
                            #{userName},
                            #{jobCode},
                            #{masterFlag},
                            #{status},
                            #{orgCode},
                            #{siteCode},
                            #{createUser},
                            #{createUserName},
                            #{updateUser},
                            #{updateUserName},
                            #{createTime},
                            #{updateTime},
                            #{yn},
                            #{ts}
                    )
    </insert>
</mapper>