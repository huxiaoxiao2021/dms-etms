<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.1.xsd">


   <bean id="miniStoreBindRelationDao" class="org.mybatis.spring.mapper.MapperFactoryBean">
        <property name="mapperInterface" value="com.jd.bluedragon.distribution.ministore.dao.MiniStoreBindRelationDao"/>
        <property name="sqlSessionFactory" ref="dynamicSqlSessionFactory"></property>
        <property name="sqlSessionTemplate" ref="dynamicSqlSessionTemplate"></property>
    </bean>

    <bean id="sortingDao" class="com.jd.bluedragon.distribution.sorting.dao.SortingDao">
        <property name="sqlSession" ref="sessionTemplate"/>
    </bean>



</beans>
