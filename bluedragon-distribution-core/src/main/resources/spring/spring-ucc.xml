<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
	http://www.springframework.org/schema/beans/spring-beans-3.0.xsd ">

	<!-- ucc 配置 -->
	<bean id="uccConfigInsPostProcessor" class="com.jd.coo.ucc.client.service.UccConfigInsPostProcessor"/>
	
	<bean id="uccConfigCenter" class="com.jd.coo.ucc.client.service.UccConfigCenter" lazy-init="false">
		<constructor-arg index="0" ref="zkConfig"/>
		<constructor-arg index="1" ref="uccConfig"/>
	</bean>
	<bean id="zkConfig" class="com.jd.coo.ucc.client.config.UccZkConfig">
		<property name="server" value="${zk.server}"/>
		<property name="sessionTimeout" value="${zk.session.timeout}"/>
		<property name="connectionTimeout" value="${zk.connection.timeout}"/>
		<property name="appPath" value="${app.path}"/>
		<property name="appGroup" value="${app.group}"/>
	</bean>
	
	<bean id="uccConfig" class="com.jd.coo.ucc.client.config.UccConfig">
		<property name="propertyConfig" ref="propertyConfig"/>
		<property name="logConfig" ref="log2Config"/>
	</bean>


<!--	log4j2日志级别控制-->
	<bean id="logConfigProcessor" class="com.jd.coo.ucc.client.plugin.processor.Log2ConfigProcessor"/>
	<bean id="log2Config" class="com.jd.coo.ucc.client.config.UccLogConfig">
		<property name="processor" ref="logConfigProcessor"/>
		<property name="weakList">
			<list>
				<value>log4j2.AsyncRoot.rootAsyncLogger</value>
			</list>
		</property>
	</bean>

	<bean name = "uccPropertyConfiguration" class="com.jd.bluedragon.configuration.ucc.UccPropertyConfiguration">
		<property name="asynbufferEnabledTaskType" value="${asynBuffer.enabled.task.type}"/>
		<property name="cassandraGlobalSwitch" value="${cassandra.global.on}"/>
		<property name="asynBufferDynamicProducerProducerType" value="${asynBuffer.dynamicProducer.producerType}"/>
		<property name="asynBufferNotenabledTaskKeyword1" value="${asynBuffer.notenabled.task.keyword1}"/>
		<property name="asynBufferJmqComsumerTaskProcessorPostTaskStoreEnbaled" value="${asynBuffer.jmqComsumer.task.processor.post.task.store.enbaled}"/>
		<property name="redisSwitchOn" value="${sys.config.task.redisSwitchOn:0}"/>
		<property name="migrationDbBackupReplicateIgnoreExp" value="${migration.db.backup.replicate.ignoreExp}"/>
		<property name="migrationDbBackupReplicateEnable" value="${migration.db.backup.replicate.enable}"/>
		<property name="workerFetchWithoutFailedTable" value="${worker.fetch.without.failed.table}"/>
		<property name="waybillSplitPageSize" value="${waybill.split.page.size}"/>
		<property name="sortingServiceMode" value="${sorting.service.mode}"/>
		<property name="chuguanNewInterfaceInsertSwitch" value="${chuguanNewInterfaceInsertSwitch:false}"/>
		<property name="chuguanNewInterfaceQuerySwitch" value="${chuguanNewInterfaceQuerySwitch:false}"/>
		<property name="sortingQueryMode" value="${sorting.query.mode}"/>
        <property name="chuguanNewPageQuerySwitch" value="${chuguanNewPageQuerySwitch:false}"/>
        <property name="preOutZoneSwitch" value="${preOutZoneSwitch:true}"/>
        <property name="logToBusinessLogByKafka" value="${logtobusinesslog.global.on}"/>
        <property name="newLogPageTips" value="${newLogPageTips:''}"/>
        <property name="oldLogPageTips" value="${newLogPageTips:''}"/>
		<property name="offlineTaskOperateTimeCorrectHours" value="${offlineTaskOperateTimeCorrectHours:24}"/>
		<property name="automaticWeightVolumeExchangeSwitch" value="${automaticWeightVolumeExchangeSwitch:false}"/>
		<property name="automaticWeightVolumeExchangeSiteCode" value="${automaticWeightVolumeExchangeSiteCode:''}"/>
        <property name="sealVolumeCheckSites" value="${sealVolumeCheckSites:-1}"/>
        <property name="disablePdaOldLogin" value="${pda.disableOldLogin:false}"/>
		<property name="switchVerToWebSites" value="${switchVerToWebSites}"/>
        <property name="collectionAddressSiteCodes" value="${collectionAddressSiteCodes}"/>
		<property name="boxLimitSites" value="${boxLimitSites:''}"/>
		<property name="clientPrintQueryGapTime" value="${client.print.query.gap.time:10}"/>
		<property name="collectGoodsDeleteSites" value="${collectGoodsDeleteSites}"/>
        <property name="inspectionBigWaybillEffectiveSites" value="${inspectionBigWaybillEffectiveSites}" />
        <property name="offlineLogGlobalSwitch" value="${offlineLogGlobalSwitch}" />
        <property name="systemLogGlobalSwitch" value="${systemLogGlobalSwitch}" />
		<property name="weightVolumeFilterWholeCountryFlag" value="${weightVolumeFilterWholeCountryFlag}" />
		<property name="singleSendSwitchVerToWebSites" value="${singleSendSwitchVerToWebSites}" />
        <property name="clickIntervalSecond" value="${clickIntervalSecond}" />
		<property name="boardCombinationSwitchVerToWebSites" value="${boardCombinationSwitchVerToWebSites}" />
		<property name="economicNetValidateWeightSwitch" value="${economicNetValidateWeightSwitch:true}"/>
		<property name="controlCheckPackage" value="${controlCheckPackage:true}"/>
		<property name="controlCheckRoute" value="${controlCheckRoute:true}"/>
		<property name="dellCancelDeliveryCheckSealCar" value="${dellCancelDeliveryCheckSealCar}" />
		<property name="removeEmptyBatchCode" value="${removeEmptyBatchCode}"/>
		<property name="loadScanTaskWaybillSize" value="${loadScanTaskWaybillSize:200}"/>
		<property name="loadScanTaskPackageSize" value="${loadScanTaskPackageSize:100}"/>
		<property name="loadScanTaskPackageMaxSize" value="${loadScanTaskPackageMaxSize:100}"/>
		<property name="preSealVehicleRemoveEmptyBatchCode" value="${preSealVehicleRemoveEmptyBatchCode:1}"/>
        <property name="inspectionAggEffectiveSites" value="${inspectionAggEffectiveSites}" />
        <property name="quickSealTips" value="${quickSealTips:''}" />
		<property name="sendCodeGenSwitchOn" value="${sendCodeGenSwitchOn:false}" />

	</bean>

	<bean id="propertyConfigProcessor" class="com.jd.coo.ucc.client.plugin.processor.PropertyConfigProcessor"/>
	<bean id="propertyConfig" class="com.jd.coo.ucc.client.config.UccPropertyConfig">
		<property name="processor" ref="propertyConfigProcessor" />
		<property name="weakList">
			<list>
				<value>uccPropertyConfiguration.asynbufferEnabledTaskType</value>
				<value>uccPropertyConfiguration.cassandraGlobalSwitch</value>
                <value>uccPropertyConfiguration.asynBufferDynamicProducerProducerType</value>
                <value>uccPropertyConfiguration.asynBufferNotenabledTaskKeyword1</value>
                <value>uccPropertyConfiguration.asynBufferJmqComsumerTaskProcessorPostTaskStoreEnbaled</value>
                <value>uccPropertyConfiguration.redisSwitchOn</value>
                <value>uccPropertyConfiguration.migrationDbBackupReplicateIgnoreExp</value>
                <value>uccPropertyConfiguration.migrationDbBackupReplicateEnable</value>
                <value>uccPropertyConfiguration.workerFetchWithoutFailedTable</value>
				<value>uccPropertyConfiguration.waybillSplitPageSize</value>
                <value>uccPropertyConfiguration.sortingServiceMode</value>
                <value>uccPropertyConfiguration.chuguanNewInterfaceInsertSwitch</value>
                <value>uccPropertyConfiguration.chuguanNewInterfaceQuerySwitch</value>
				<value>uccPropertyConfiguration.sortingQueryMode</value>
				<value>uccPropertyConfiguration.chuguanNewPageQuerySwitch</value>
				<value>uccPropertyConfiguration.preOutZoneSwitch</value>
				<value>uccPropertyConfiguration.logToBusinessLogByKafka</value>
				<value>uccPropertyConfiguration.newLogPageTips</value>
				<value>uccPropertyConfiguration.oldLogPageTips</value>
				<value>uccPropertyConfiguration.offlineTaskOperateTimeCorrectHours</value>
				<value>uccPropertyConfiguration.automaticWeightVolumeExchangeSwitch</value>
				<value>uccPropertyConfiguration.automaticWeightVolumeExchangeSiteCode</value>
				<value>uccPropertyConfiguration.sealVolumeCheckSites</value>
				<value>uccPropertyConfiguration.switchVerToWebSites</value>
				<value>uccPropertyConfiguration.disablePdaOldLogin</value>
				<value>uccPropertyConfiguration.collectionAddressSiteCodes</value>
				<value>uccPropertyConfiguration.clientPrintQueryGapTime</value>
				<value>uccPropertyConfiguration.boxLimitSites</value>
				<value>uccPropertyConfiguration.collectGoodsDeleteSites</value>
                <value>uccPropertyConfiguration.inspectionBigWaybillEffectiveSites</value>
                <value>uccPropertyConfiguration.offlineLogGlobalSwitch</value>
                <value>uccPropertyConfiguration.systemLogGlobalSwitch</value>
				<value>uccPropertyConfiguration.weightVolumeFilterWholeCountryFlag</value>
				<value>uccPropertyConfiguration.singleSendSwitchVerToWebSites</value>
                <value>uccPropertyConfiguration.clickIntervalSecond</value>
				<value>uccPropertyConfiguration.boardCombinationSwitchVerToWebSites</value>
				<value>uccPropertyConfiguration.economicNetValidateWeightSwitch</value>
				<value>uccPropertyConfiguration.controlCheckPackage</value>
				<value>uccPropertyConfiguration.controlCheckRoute</value>
				<value>uccPropertyConfiguration.dellCancelDeliveryCheckSealCar</value>
				<value>uccPropertyConfiguration.removeEmptyBatchCode</value>
				<value>uccPropertyConfiguration.loadScanTaskWaybillSize</value>
				<value>uccPropertyConfiguration.loadScanTaskPackageSize</value>
				<value>uccPropertyConfiguration.loadScanTaskPackageMaxSize</value>
				<value>uccPropertyConfiguration.preSealVehicleRemoveEmptyBatchCode</value>
                <value>uccPropertyConfiguration.inspectionAggEffectiveSites</value>
                <value>uccPropertyConfiguration.quickSealTips</value>
				<value>uccPropertyConfiguration.sendCodeGenSwitchOn</value>
			</list>
		</property>
	</bean>
</beans>