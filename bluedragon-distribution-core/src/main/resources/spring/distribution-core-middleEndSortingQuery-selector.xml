<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:jmq="http://code.jd.com/schema/jmq"
       xmlns:util="http://www.springframework.org/schema/util"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans.xsd
        http://code.jd.com/schema/jmq
	    http://code.jd.com/schema/jmq/jmq-1.1.xsd
	    http://www.springframework.org/schema/util
	    http://www.springframework.org/schema/util/spring-util.xsd">
	    
	    <bean id="dmsSortingQueryDao" class="com.jd.bluedragon.distribution.middleend.DmsSortingDao"></bean>
	    <bean id="middleEndSortingQueryDao" class="com.jd.bluedragon.distribution.middleend.MiddleEndSortingDao"></bean>
	    <bean id="failoverSortingQueryDao" class="com.jd.bluedragon.distribution.middleend.FailoverSortingDao"></bean>

	    <!-- 动态可管理任务生产者配置。 -->
	    <bean id="dynamicSortingQueryDao" class="com.jd.bluedragon.distribution.task.asynBuffer.DmsDynamicProducer">
	    	<property name="jmqProducer" ref="asynBufferJmqProducer"/>
	    	<property name="tbscheduleProducer" ref="tbscheduleProducer"/>
	    	<property name="failoverProducer" ref="asynBufferFailoverProducer"/>
	    	<property name="producerType" value="FAILOVER"></property>
	    </bean>
	    
</beans>