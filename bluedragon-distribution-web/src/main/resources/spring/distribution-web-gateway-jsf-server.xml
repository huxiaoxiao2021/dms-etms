<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:jsf="http://jsf.jd.com/schema/jsf"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd
    http://jsf.jd.com/schema/jsf http://jsf.jd.com/schema/jsf/jsf.xsd">

    <!-- 分拣 -->
    <bean id="sortingGatewayService" class="com.jd.bluedragon.distribution.external.gateway.service.impl.SortingGatewayServiceImpl"/>
    <jsf:provider id="jsfSortingService"
                  interface="com.jd.bluedragon.distribution.external.gateway.service.SortingGatewayService"
                  alias="${dms.gateway.jsf.alias}"
                  ref="sortingGatewayService" server="dmsjsf">
    </jsf:provider>

    <bean id="inspectionGatewayService" class="com.jd.bluedragon.distribution.external.gateway.service.impl.InspectionGatewayServiceImpl"/>
    <jsf:provider id="jsfInspectionGatewayService"
                  interface="com.jd.bluedragon.distribution.external.gateway.service.InspectionGatewayService"
                  alias="${dms.gateway.jsf.alias}"
                  ref="inspectionGatewayService" server="dmsjsf">
    </jsf:provider>

</beans>
