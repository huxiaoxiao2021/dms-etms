<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:aop="http://www.springframework.org/schema/aop"
	xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="
	http://www.springframework.org/schema/beans 
	http://www.springframework.org/schema/beans/spring-beans-3.1.xsd 
    http://www.springframework.org/schema/aop
	http://www.springframework.org/schema/aop/spring-aop-3.1.xsd
    http://www.springframework.org/schema/context
	http://www.springframework.org/schema/context/spring-context-3.1.xsd">
	
	<context:property-placeholder location="classpath:configured/wss-service.properties" ignore-unresolvable="false"/>
	
	<bean id="serviceSetting" class="com.jd.wss.rpc.ServiceSetting">
		<property name="centerDataSource" ref="centerDataSource"/>
		<property name="application" value="${serviceSetting.application}"/>
		<property name="baseUri" value="${serviceSetting.baseUri}"/>
		<property name="token" value="${serviceSetting.token}"/>
		<property name="domain" value="${serviceSetting.domain}" />
		<property name="ownSign" value="${serviceSetting.ownSign}" />		
	</bean>
	
	<bean id="messageServiceSetting" class="com.jd.wss.rpc.ServiceSetting">
        <property name="centerDataSource" ref="centerDataSource"/>
        <property name="application" value="${serviceSetting.application}"/>
        <property name="baseUri" value="${serviceSetting.baseUri}"/>
        <property name="token" value="${serviceSetting.token}"/>
        <property name="domain" value="${serviceSetting.domain}" />
        <property name="ownSign" value="${serviceSetting.ownSign.message}" />        
    </bean>
    
