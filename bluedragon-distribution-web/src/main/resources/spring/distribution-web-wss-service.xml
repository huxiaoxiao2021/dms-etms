	<bean id="distributionService" class="com.jd.bluedragon.distribution.wss.service.impl.DistributionWssServiceImpl" />

	<bean name="/distributionService" class="com.jd.wss.rpc.factory.ServiceExproterFactory">
		<property name="setting" ref="serviceSetting"/>
		<property name="protocols" value="${distributionService.protocols}"/>
		<property name="ports" value="${distributionService.ports}"/>
		<property name="version" value="${distributionService.version}"/>
		<property name="serviceName" value="distributionService"/>
		<property name="service" ref="distributionService"/>
		<property name="serviceInterface" value="com.jd.bluedragon.distribution.wss.service.DistributionWssService"/>
		<property name="remark" value="分拣中心相关服务"/>
	</bean>
	
	<bean id="vehicleBoxService" class="com.jd.bluedragon.distribution.wss.service.impl.SealVehicleBoxServiceImpl" />

	<bean name="/vehicleBoxService" class="com.jd.wss.rpc.factory.ServiceExproterFactory">
		<property name="setting" ref="serviceSetting"/>
		<property name="protocols" value="${vehicleBoxService.protocols}"/>
		<property name="ports" value="${vehicleBoxService.ports}"/>
		<property name="version" value="${vehicleBoxService.version}"/>
		<property name="serviceName" value="vehicleBoxService"/>
		<property name="service" ref="vehicleBoxService"/>
		<property name="serviceInterface" value="com.jd.bluedragon.distribution.wss.service.SealVehicleBoxService"/>
		<property name="remark" value="封车封箱相关服务"/>
	</bean>
	
