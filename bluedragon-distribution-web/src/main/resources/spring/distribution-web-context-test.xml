<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	   xmlns:context="http://www.springframework.org/schema/context"
	   xsi:schemaLocation="http://www.springframework.org/schema/beans
	http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
    http://www.springframework.org/schema/context
	http://www.springframework.org/schema/context/spring-context-3.1.xsd">

	<context:component-scan base-package="com.jd.autotest.controller" />

	<!-- autoTest配置缓存注入的id；数据源id -->
	<bean id="jimClientId" class="java.lang.String">
		<constructor-arg value="jimClient" />
	</bean>
	<bean id="dataSourceId" class="java.lang.String">
		<constructor-arg value="dmsReadDataSource" />
	</bean>
</beans>
