<body>
<input type="hidden" id="contextPath" value="#springUrl('')"/>
<script type="text/javascript" src="#springUrl('')/static/js/jquery-plugins/jquery.autocomplete.js"></script>
<script type="text/javascript" src="#springUrl('')/static/js/bootstrap/bootstrap-treeview.js"></script>
<link rel="stylesheet" type="text/css" href="#springUrl('')/static/css/jquery.autocomplete.css"/>
<script type="text/javascript" src="#springUrl('/static/js/areadest/list.js')"></script>
<div id="breadcrumb">
    <p><a href="#">分拣中心</a>&nbsp;&gt;&nbsp;<a href="#">区域批次目的地配置表</a></p>
</div>
<!--[if !ie]>breadcrumb<![endif]-->
<div id="content" width="100%">
    <div class="m">
        <div class="tbl-n">
            <table border="0" cellpadding="0" cellspacing="0" width="100%">
                <tbody>
                <tr>
                    <td align="right">始发分拣中心：</td>
                    <td>
                        <input type="text" id="createSiteName" name="createSiteName" maxlength="50" disabled="true"
                               style="width:150px" value="$!{createSiteName}"/>
                        <input type="hidden" id="createSiteCode" name="createSiteCode" value="$!{createSiteCode}"/>
                    </td>
                    <td align="right">中转分拣中心：</td>
                    <td>
                        <select id="transferSite" style="width:150px">
                            #if($!{transferSiteCode})
                                <option value="$!{transferSiteCode}">$!{transferSiteCode}
                                    &nbsp$!{transferSiteName}</option>
                            #else
                                <option value="-1">所有分拣中心</option>
                            #end
                        </select>
                    </td>
                    <td align="right">批次目的地：</td>
                    <td>
                        <select id="receiveSite" style="width:150px">
                            #if($!{receiveSiteCode})
                                <option value="$!{receiveSiteCode}">$!{receiveSiteCode}
                                    &nbsp$!{receiveSiteName}</option>
                            #else
                                <option value="-1">所有分拣中心</option>
                            #end
                        </select>
                    </td>
                    <td>
                        <input type="button" id="queryBtn" name="search" value="查询" class="btn_c"/>
                        <input type="button" id="funReset" value="重置" class="btn_c"/>
                        <input type="button" id="addBtn" name="add" value="新增" class="btn_c"/>
                    </td>
                </tr>
                </tbody>
            </table>
        </div>
    </div>
    <!--[if !ie]>tbl-n<![endif]-->
    <div id="opening">
        <div class="tbl-list">
            <div class="tbl-bg">
                <table id="paperTable" cellSpacing=0 cellPadding=0 width="100%">
                    <tbody>
                    #*<tr>
                        <td style="text-align: left;">
                            <div class="form-group">
                                <div id="myAlert" class="alert alert-warning">
                                    <a href="#" class="close" data-dismiss="alert">&times;</a>
                                    <strong>警告！</strong>您的网络连接有问题。
                                </div>
                            </div>
                        </td>
                    </tr>*#
                    <tr>
                        <td style="text-align: left;">
                            <div class="form-group">
                                <button type="button" class="btn btn-success" id="btn-expand-all">展开所有</button>
                                <button type="button" class="btn btn-danger" id="btn-collapse-all">关闭所有</button>
                            </div>
                        </td>
                    </tr>
                    <tr>
                        <td>
                            <div class="form-group">
                                <input type="input" class="form-control" id="input-select-node"
                                       placeholder="输入分拣中心名称..." value="" style="width:500px">
                            </div>
                        </td>
                    </tr>
                    <tr>
                        <td style="text-align: left;">
                            <div class="form-group">
                                <div id="treeview" class="" style="width:500px"></div>
                            </div>
                        </td>
                    </tr>
                    </tbody>
                </table>
            </div>
        </div>
    </div>
</div>
</body>
</html>
