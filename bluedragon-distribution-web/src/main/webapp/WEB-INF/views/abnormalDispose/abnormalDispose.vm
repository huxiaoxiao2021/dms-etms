<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <title>分拣批次清零</title>
    <meta http-equiv="pragma" content="no-cache"/>
    <meta http-equiv="cache-control" content="no-cache">
    <meta http-equiv="expires" content="0">
    #parse("system/common/baseImport.vm")
    <script src="/static/js/abnormalDispose/abnormalDispose.js"></script></head>
<body class="container-fluid">
<div id="dataTableMainDiv" >
    <ol class="breadcrumb">
        <li><a href="#">分拣中心</a></li>
        <li class="active">分拣批次清零</li>
    </ol>
    <div id="query-condition">
        <form id="query-form" class="form-horizontal">
            <fieldset>
                <div class="row">
                    <div class="col-xs-12">
                        <div class="col-xs-4">
                            <div class="form-group">
                                <label class="control-label col-xs-3 " for="startDate">查询日期</label>
                                <div class=" col-xs-4">
                                    <input type="text" id="startTime" name="startTime" class="form-control search-param ">

                                </div>
                                <label class="control-label col-xs-1 " for="startDate" style="text-align: center" >—</label>
                                <div class=" col-xs-4">
                                    <input type="text" id="endTime" name="endTime" class="form-control search-param " >
                                </div>
                            </div>
                        </div>
                        <div class="col-xs-8">
                            <div class="col-xs-3">
                                <div class="form-group">
                                    <label class="control-label col-xs-4" for="site-select">区域 </label>
                                    <div class=" col-xs-8">
                                        <select id="areaId" class="form-control search-param " name="areaId"
                                                value=""></select>
                                    </div>
                                </div>
                            </div>
                            <div class="col-xs-2">
                                <div class="form-group">
                                    <label class="control-label col-xs-3" for="site-select">省 </label>
                                    <div class=" col-xs-9">
                                        <select id="provinceId" class="form-control search-param " name="provinceId"
                                                value=""></select>
                                    </div>
                                </div>
                            </div>
                            <div class="col-xs-3">
                                <div class="form-group">
                                    <label class="control-label col-xs-4" for="site-select">市 </label>
                                    <div class=" col-xs-8">
                                        <select id="cityId" class="form-control search-param " name="cityId"
                                                value=""></select>
                                    </div>
                                </div>
                            </div>
                            <div class="col-xs-4">
                                <div class="form-group">
                                    <label class="control-label col-xs-4" for="site-select">分拣中心 </label>
                                    <div class=" col-xs-8">
                                        <select id="dmsSiteCode" class="form-control search-param " name="dmsSiteCode"
                                                value=""></select>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </fieldset>
        </form>
        <div id="toolbar" class="btn-group">
            <button id="btn_query" type="button" class="btn btn-default">
                <span class="glyphicon glyphicon-search" aria-hidden="true"></span>查询
            </button>
            <button id="btn_add" type="button" class="btn btn-default">
                <span class="glyphicon glyphicon-plus" aria-hidden="true"></span>新增
            </button>
            <button id="btn_edit" type="button" class="btn btn-default">
                <span class="glyphicon glyphicon-pencil" aria-hidden="true"></span>修改
            </button>
        </div>
        <table id="dataTable"></table>
    </div>

</div>

</body>
</html>