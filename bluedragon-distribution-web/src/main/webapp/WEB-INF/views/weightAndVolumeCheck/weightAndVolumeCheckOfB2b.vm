    <script src="/static/js/weightAndVolumeCheck/weightAndVolumeCheckOfB2b.js"></script>
    <script src="/static/imports/bootstrap/v3.3.7/plugin/bootstrap3-editable/js/bootstrap-editable.min.js"></script>
    <script src="/static/js/weightAndVolumeCheck/bootstrap-table-editable.js"></script>
</head>
<body>
<input type="hidden" id="createSiteCode" value="$!{createSiteCode}"/>
<input type="hidden" id="loginErp" value="$!{loginErp}"/>
<div id="dataTableDiv" class="main">
        <ol class="breadcrumb">
            <li><a href="#">分拣中心</a></li>
            <li class="active">B网转运重量抽检</li>
        </ol>
		<!-- 查询条件 -->
        <div id="query-condition">
            <form id="query-form" class="form-horizontal">
                <fieldset>

                    <div class="row">
                        <div class="col-xs-12">
                            <div class="col-xs-3">
                                <div class="form-group">
                                    <label class="control-label col-xs-3" for="box-code-input"></label>
                                    <div class="col-xs-4" >
                                        <label for="checkOfWaybill">
                                        <input type="radio" id="checkOfWaybill" class="control-label" name="weightCheck" checked>按运单抽检
                                    </div>
                                    <div class="col-xs-4" >
                                        <label for="checkOfPackage">
                                        <input type="radio" id="checkOfPackage" name="weightCheck" class="control-label">按包裹抽检
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>

                    <div class="row">
                        <div class="col-xs-12">
                            <div class="col-xs-4">
                                <div class="form-group">
                                    <label class="control-label col-xs-4">运单号/包裹号：</label>
                                    <div class="col-xs-6">
                                        <input type="text" id="waybillOrPackageCode" name="waybillOrPackageCode" class="form-control search-param" >
                                    </div>
                                </div>
                            </div>
                            <div id="waybillWeightIsShow" class="col-xs-3">
                                <div class="form-group">
                                    <label class="control-label col-xs-5">运单总重量(公斤)：</label>
                                    <div class="input-group spinner">
                                        <input type="text" id="waybillWeight" class="form-control" style="text-align: center" value="0.00">
                                        <div class="input-group-btn-vertical">
                                            <button id="waybillWeightUp" class="btn btn-default" type="button"><i class="glyphicon glyphicon-chevron-up"></i></button>
                                            <button id="waybillWeightDown" class="btn btn-default" type="button"><i class="glyphicon glyphicon-chevron-down"></i></button>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div id="waybillVolumeIsShow" class="col-xs-3">
                                <div class="form-group">
                                    <label class="control-label col-xs-5">运单总体积(立方米)：</label>
                                    <div class="input-group spinner">
                                        <input type="text" id="waybillVolume" class="form-control" style="text-align: center" value="0.000000">
                                        <div class="input-group-btn-vertical">
                                            <button id="waybillVolumeUp" class="btn btn-default" type="button"><i class="glyphicon glyphicon-chevron-up"></i></button>
                                            <button id="waybillVolumeDown" class="btn btn-default" type="button"><i class="glyphicon glyphicon-chevron-down"></i></button>
                                        </div>
                                    </div>
                                </div>
                            </div>

                            <div id="toolbar" class="btn-group">
                                <button id="btn_weightVolumeRatio_check" type="button" class="btn btn-primary">
                                    <span class="glyphicon glyphicon-check" aria-hidden="true"></span>&nbsp;重泡比校验
                                </button>
                            </div>
                            <div id="toolbar" class="btn-group">
                                <button id="btn_check" type="button" class="btn btn-primary">
                                    <span class="glyphicon glyphicon-check" aria-hidden="true"></span>&nbsp;检查
                                </button>
                            </div>
                            <div id="toolbar" class="btn-group">
                                <button id="btn_submit" type="button" class="btn btn-primary">
                                    <span class="glyphicon glyphicon-submit" aria-hidden="true"></span>&nbsp;提交
                                </button>
                            </div>
                            <div id="toolbar" class="btn-group">
                                <button id="btn_refresh" type="button" class="btn btn-primary">
                                    <span class="glyphicon glyphicon-refresh" aria-hidden="true"></span>&nbsp;重置
                                </button>
                            </div>

                        </div>
                    </div>

                </fieldset>
            </form>
        </div>
        <hr />
    <div id="waybillDataTableDiv" style="display: block">
        <table id="waybillDataTable" class="waybillDataTable" width="83%">
        </table>
    </div>
    <div id="packageDataTableDiv" style="display: none">
        <table id="packageDataTable" width="83%">
        </table>
    </div>

</div>
