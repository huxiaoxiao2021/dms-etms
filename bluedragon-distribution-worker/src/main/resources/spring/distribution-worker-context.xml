<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:context="http://www.springframework.org/schema/context"
    xsi:schemaLocation="
        http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-3.1.xsd
        http://www.springframework.org/schema/context
        http://www.springframework.org/schema/context/spring-context-3.1.xsd">
	

	<import resource="classpath:spring/distribution-core-context.xml" />
	<import resource="classpath:spring/distribution-worker-task.xml" />
	<import resource="classpath:spring/distribution-worker-task-redis.xml" />
	<import resource="classpath:spring/distribution-worker-wss.xml"/>
	
	<!-- ump monitor begin -->
	<!--  监控注解处理器 -->
	<bean class="com.jd.ump.annotation.JAnnotation">
	   <property name="jvmKey" value="Bluedragon_dms_worker.jvm"></property>
	   <property name="systemKey" value="Bluedragon_dms_worker.system"></property>
	</bean>
	<!-- ump monitor end -->
</beans> 
